Algoritmo "VARIAVELINTEIRO"

Var
// Nomes das Variaveis

   A : inteiro
   B, C : inteiro

Inicio
      //fase entrada de dados
   // Comando de saida
   escreva("Digite um numero: ")
   
   //  Comando de entrada
   leia(A)
   
   escreva("Digite outro numero: ")
   leia(B)
   
   // processamento
   C <- A + B
   
     // fase saida de dados
   escreva(C)

Fimalgoritmo