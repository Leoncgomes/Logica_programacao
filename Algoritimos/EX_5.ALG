Algoritmo "EX_5"

Var
// Seção de Declarações das variáveis 
     n1, n2 : real
     resp : real

Inicio
      // Entrada de dados
      escreva("Digite um numero: ")
      leia(n1)
      
      escreva("Digite um numero: ")
      leia(n2)
      
      // Processamento
      resp <- n1 / n2
      
      // Saida de dado
      escreval("Divisão: ", resp)

Fimalgoritmo