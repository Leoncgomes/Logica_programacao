Algoritmo "EX_6"

Var
// Seção de Declarações das variáveis 
     n1, n2, resp : real


Inicio
      // Entrada de dados
      escreva("Digite um número: ")
      leia(n1)
      
      escreva("Digite um número: ")
      leia(n2)
      
      // Processamento
      resp <- n1 / n2
      
      // Saida de dados
      escreval("Resultado: ", resp)


Fimalgoritmo